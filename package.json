{
  "name": "funda-server-side",
  "version": "1.3.0",
  "description": "Project for Funda, but this time rendered server side",
  "main": "sever.js",
  "scripts": {
    "start": "node server.js",
    "start-dev": "nodemon server.js",
    "build": "browserify --node src/js/app.js -o public/funda.js",
    "watch-build": "watchify src/js/app.js -d -o public/funda.js -v",
    "minify": "esmangle public/funda.js -o public/funda.min.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IanCStewart/funda-server-side.git"
  },
  "keywords": [],
  "author": "Ian Stewart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IanCStewart/funda-server-side/issues"
  },
  "homepage": "https://github.com/IanCStewart/funda-server-side#readme",
  "devDependencies": {
    "ascii-art": "^1.4.2",
    "browserify": "^14.1.0",
    "eslint": "^3.15.0",
    "esmangle": "^1.0.1",
    "nodemon": "^1.11.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "node-fetch": "^1.6.3"
  }
}
